<Files .env>
    Order allow,deny
    Deny from all
</Files>

<Files .zip>
    Order allow,deny
    Deny from all
</Files>

<IfModule mod_security.c>
  SecFilterEngine Off
  SecFilterScanPOST Off
</IfModule>

<FilesMatch 'service-account-credentials.json'>
  Order allow,deny
  Deny from all
</FilesMatch>

